<% layout("/layouts/boilerplate") %>
<% if(statuscode===500){ %>
 <div class="heading">
  <h1 class="fw-bold  text-center">Page not found (404)</h1>
    </div>

<div class="text-center me-3 mt-5">
 <img src="/image/pagenotfound.svg">
 </div>
 <div class="text-center mt-3 message" >
    <h3>The page you were looking for doesn't exist.</h3>
 </div>

 <div class="text-center mt-3 message">
    <h5>You may have mistyped the address or the page may have moved.</h5>
 </div>
 <%}%>

 <% if(statuscode===400){ %>
<div class="row mt-4 offset-2">
    <div class="alert alert-danger col-8" role="alert" style="height: 60px;" >
 <p class="alert-heading"><%= message %></p>
</div>
</div>
 <%}%>